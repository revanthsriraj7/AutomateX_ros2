<?xml version="1.0" ?>
<robot name="AutomateX" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Material Definition -->
  <material name="orange">
    <color rgba="0.996 0.580 0.176 1.0"/>
  </material>

  <!-- World Frame -->
  <link name="world"/>
  
  <!-- Link 0 - Base -->
  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/base_link.obj" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/base_link.obj" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
      <inertial>
          <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
          <mass value="0.1"/>
          <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
      </inertial>
  </link>

  <!-- World-Base Joint -->
  <joint name="virtual_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <!-- Link 1 -->
  <link name="base_plate">
    <visual>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/base_plate.obj" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/base_plate.obj" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <mass value="0.1"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
  </link>

  <!-- Joint 1 -->
  <joint name="arm_joint1" type="revolute">
    <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-6.2832" soft_upper_limit="6.2832"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="base_link"/>
    <child link="base_plate"/>
    <axis xyz="0 0 1"/>
    <limit effort="150.0" lower="-6.2832" upper="6.2832" velocity="3.15"/>
  </joint>

  <!-- Link 2 -->
  <link name="lower_arm">
    <visual>
      <origin xyz="0 0 -0.307026" rpy="0.0 0.0 0.0"/>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/lower_arm.obj" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/lower_arm.obj" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
        <inertial>
          <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
          <mass value="0.1"/>
          <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
      </inertial>
  </link>

  <!-- Joint 2 -->
  <joint name="arm_joint2" type="revolute">
    <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-6.2832" soft_upper_limit="6.2832"/>
    <origin rpy="0 0 0" xyz="0 0 0.307026"/>
    <parent link="base_plate"/>
    <child link="lower_arm"/>
    <axis xyz="0 1 0"/>
    <limit effort="150.0" lower="-6.2832" upper="6.2832" velocity="3.15"/>
  </joint>

  <!-- Link 3 -->
  <link name="upper_arm">
    <visual>
      <origin xyz="0 0 -0.550278" rpy="0.0 0.0 0.0"/>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/upper_arm.obj" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/upper_arm.obj" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
      <inertial>
          <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
          <mass value="0.1"/>
          <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
      </inertial>
  </link>

  <!-- Joint 3 -->
  <joint name="arm_joint3" type="revolute">
    <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-3.1416" soft_upper_limit="3.1416"/>
    <origin rpy="0 0 0" xyz="0 0 0.243252"/>
    <parent link="lower_arm"/>
    <child link="upper_arm"/>
    <axis xyz="0 1 0"/>
    <limit effort="150.0" lower="-3.1416" upper="3.1416" velocity="3.15"/>
  </joint>

  <!-- Link 4 -->
  <link name="wrist_1">
    <visual>
      <origin xyz="0 0 -0.800024" rpy="0.0 0.0 0.0"/>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/wrist_1.obj" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/wrist_1.obj" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
      <inertial>
          <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
          <mass value="0.1"/>
          <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
      </inertial>
  </link>

  <!-- Joint 4 -->
  <joint name="arm_joint4" type="revolute">
    <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-6.2832" soft_upper_limit="6.2832"/>
    <origin rpy="0 0 0" xyz="0 0 0.249746"/>
    <parent link="upper_arm"/>
    <child link="wrist_1"/>
    <axis xyz="0 1 0"/>
    <limit effort="54.0" lower="-6.2832" upper="6.2832" velocity="3.2"/>
  </joint>

  <!-- Link 5 -->
  <link name="wrist_2">
    <visual>
      <origin xyz="0 -0.122627 -0.800024" rpy="0.0 0.0 0.0"/>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/wrist_2.obj" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/wrist_2.obj" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
      <inertial>
          <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
          <mass value="0.1"/>
          <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
      </inertial>
  </link>

  <!-- Joint 5 -->
  <joint name="arm_joint5" type="revolute">
    <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-6.2832" soft_upper_limit="6.2832"/>
    <origin rpy="0 0 0" xyz="0 0.122627 0"/>
    <parent link="wrist_1"/>
    <child link="wrist_2"/>
    <axis xyz="0 0 1"/>
    <limit effort="54.0" lower="-6.2832" upper="6.2832" velocity="3.2"/>
  </joint>

  <!-- Link 6 -->
  <link name="wrist_3">
    <visual>
      <origin xyz="0 -0.122627 -0.854858" rpy="0.0 0.0 0.0"/>
      <geometry>
        <mesh filename="package://arm_description/meshes/wrist_3.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://arm_description/meshes/wrist_3.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
      <inertial>
          <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
          <mass value="0.1"/>
          <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
      </inertial>
  </link>

  <!-- Joint 6 -->
  <joint name="arm_joint6" type="revolute">
    <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-6.2832" soft_upper_limit="6.2832"/>
    <origin rpy="0 0 0" xyz="0 0 0.123876"/>
    <parent link="wrist_2"/>
    <child link="wrist_3"/>
    <axis xyz="0 1 0"/>
    <limit effort="54.0" lower="-6.2832" upper="6.2832" velocity="3.2"/>
  </joint>


  <!-- Hand Link -->
  <link name="panda_hand">
    <visual>
      <geometry>
        <mesh filename="package://arm_description/meshes/visual/hand.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/hand.obj"/>
      </geometry>
    </collision>
      <inertial>
          <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
          <mass value="0.1"/>
          <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
      </inertial>
  </link>

  <!-- Hand Joint -->
  <joint name="panda_hand_joint" type="fixed">
    <parent link="wrist_3"/>
    <child link="panda_hand"/>
    <origin rpy="-1.57079632679 0 0" xyz="0 0.062681 0"/>
  </joint>

  <!-- Left Finger Link -->
  <link name="panda_leftfinger">
    <visual>
      <geometry>
        <mesh filename="package://arm_description/meshes/visual/finger.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/finger.obj"/>
      </geometry>
    </collision>
      <inertial>
          <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
          <mass value="0.1"/>
          <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
      </inertial>
  </link>

  <!-- Right Finger Link -->
  <link name="panda_rightfinger">
    <visual>
      <origin rpy="0 0 3.14159265359" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://arm_description/meshes/visual/finger.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 3.14159265359" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://arm_description/meshes/collision/finger.obj"/>
      </geometry>
    </collision>
      <inertial>
          <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
          <mass value="0.1"/>
          <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
      </inertial>
  </link>

  <!-- Finger Joint 1 -->
  <joint name="panda_finger_joint1" type="prismatic">
    <parent link="panda_hand"/>
    <child link="panda_leftfinger"/>
    <origin rpy="0 0 0" xyz="0 0 0.0584"/>
    <axis xyz="0 1 0"/>
    <limit effort="30.0" lower="0.0" upper="0.04" velocity="10.0"/>
  </joint>

  <!-- Finger Joint 2 -->
  <joint name="panda_finger_joint2" type="prismatic">
    <parent link="panda_hand"/>
    <child link="panda_rightfinger"/>
    <origin rpy="0 0 0" xyz="0 0 0.0584"/>
    <axis xyz="0 -1 0"/>
    <limit effort="30.0" lower="0.0" upper="0.04" velocity="10.0"/>
    <!-- <mimic joint="panda_finger_joint1" multiplier="-1"/> -->
  </joint>


</robot>
