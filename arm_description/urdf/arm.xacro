<?xml version="1.0" ?>
<robot name="panda" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- World Frame -->
  <link name="world"/>
  
  <!-- Link 0 - Base -->
  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="package://arm_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://arm_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
      <inertial>
          <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
          <mass value="0.1"/>
          <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
      </inertial>
  </link>

  <!-- World-Base Joint -->
  <joint name="virtual_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <!-- Link 1 -->
  <link name="base_plate">
    <visual>
      <geometry>
        <mesh filename="package://arm_description/meshes/base_plate.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://arm_description/meshes/base_plate.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <mass value="0.1"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
  </link>

  <!-- Joint 1 -->
  <joint name="arm_joint1" type="revolute">
    <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-2.8973" soft_upper_limit="2.8973"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="base_link"/>
    <child link="base_plate"/>
    <axis xyz="0 0 1"/>
    <limit effort="30.0" lower="-2.9671" upper="2.9671" velocity="10.0"/>
  </joint>

  <!-- Link 2 -->
